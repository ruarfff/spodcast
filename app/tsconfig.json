{
  "compilerOptions": {
    "incremental": true,
    "isolatedModules": true,
    "importsNotUsedAsValues": "error",
    "allowJs": true,
    "jsx": "react",
    "target": "es2020",
    "strict": true,
    "resolveJsonModule": true,
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "noEmit": true
  }
}
